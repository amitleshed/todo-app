 <table id="main-table" class="table table-hover table-bordered">
      <thead>
        <tr>
          <th>Title</th>
          <th>Created at</th>
          <th>Completed</th>
        </tr>
      </thead>
      <tbody id="table-body">
        <% tasks.each do |task| %>
          <%= render task %>
        <% end %>
      </tbody>
   </table>